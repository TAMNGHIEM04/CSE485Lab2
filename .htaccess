RewriteEngine On
RewriteRule ^news/detail/([0-9]+)$ index.php?url=home/detail/$1 [QSA,L]
RewriteRule ^admin/dashboard$ index.php?url=admin/dashboard [QSA,L]
RewriteRule ^admin/news/add$ index.php?url=admin/addNews [QSA,L]
RewriteRule ^admin/news/edit/([0-9]+)$ index.php?url=admin/editNews/$1 [QSA,L]
RewriteRule ^admin/news/delete/([0-9]+)$ index.php?url=admin/deleteNews/$1 [QSA,L]
RewriteRule ^(.*)$ index.php?url=$1 [QSA,L]